#profile
  = render 'users/profile/nav_profile'
  %br.clearit
  #user-main
    = render 'users/profile/sidebar_nav'
    .tab_container
      #basic_info.tab_content
        %h2 Basic Information
        %br.clearit
        .tab_form
          = render 'users/profile/form'

      #military_info.tab_content
        %h2 Military Information
        %br.clearit
        .tab_form
          = render 'military/form'

      #skills_edu.tab_content
        %h2 Education
        %br.clearit
        .tab_form
          %p= link_to 'Add Education', new_education_path
          %p= link_to 'Edit Education', educations_path
        
        %h2 Skills
        %br.clearit
        .tab_form
          %p= link_to 'Add new skill', new_skill_path
          %p= link_to 'Edit skills', skills_path

        %h2 Languages
        %br.clearit
        .tab_form
          %p= link_to 'Add Language', new_language_path
          %p= link_to 'Edit Languages',languages_path

        %h2 Trainings
        %br.clearit
        .tab_form
          %p= link_to 'Add Training', new_training_path
          %p= link_to 'Edit Trainings', trainings_path

        %h2 Certifications
        %br.clearit
        .tab_form
          %p= link_to 'Add Certification', new_certification_path
          %p= link_to 'Edit Certifications', certifications_path

      #exp.tab_content
        %h2 Experience
        %br.clearit
        .tab_form
          %p= link_to 'Add Job', new_job_history_path
          %p= link_to 'Edit Jobs', job_histories_path

    %br.clearit

